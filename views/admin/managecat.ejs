<%- include('partials/Header') %>
<div class="container">
    <div style="margin-top: 135px;"  class="container">

<div class="row">
    <div class="col-lg-12">
        <div class="card m-b-30">
            <div class="card-body">

                <h4 class="mt-0 header-title">manage category and SubCategory</h4>
                <p class="sub-title">edit your category and subcategory 
                    <code>Here</code>
                </p>
                
                <div class="table-responsive">
                    <table class="table mb-0">
                        <thead class="thead-default">
                            <tr>
                                <th>#</th>
                                <th>Category</th>
                                <th>Sub Category</th>
                                <th>Edit</th>
                                <th>delete</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            <% let count=1%> 
<%locals.Allcategory.forEach(element => { %>
    
    <tr>
        <th scope="row"><%=count %></th>
        <th ><%=element. MainCategory %> </th>
        <td> <% element.SubCategory.forEach((e)=> { %>
<%= e %> <br>
        <% }) %> 
            </td>
            <td> <a href="/admin/editcategory?catid=<%=element._id  %> "><i class="fas fa-edit"></i></a> </td>
            <td> <a  href="" onclick="deletecategory('<%=element._id %>')"><i class="fas fa-trash-alt"></i></a> </td>
        <% count++ %> 
<% }) %> 
               
                            
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div> <!-- end col -->
</div>
    <%- include("partials/footer") %> 