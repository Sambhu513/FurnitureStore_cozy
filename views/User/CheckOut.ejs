    <form id="placeOrder" action="" method="">


    <div class="checkout-area pt-100px pb-100px">
        <div class="container">
            <div class="row">
                <div class="col-lg-7">
                    <div class="billing-info-wrap">
                        <h3>Billing Details</h3>
                        <div class="row">
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>First Name</label>
                                    <input id="firstname" name="FirstName"  type="text" required>
                                
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>Last Name</label>
                                    <input id="lastname" name="LastName" type="text" />
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="billing-info mb-4">
                                    <label>Address</label>
                                    <input id="address" name="Address" type="text" />
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="billing-info mb-4">
                                    <label>Country</label>
                                    <input id="country" name="Country" type="text" />

                                </div>
                            </div>
                           
                            <div class="col-lg-12">
                                <div class="billing-info mb-4">
                                    <label>Town / City</label>
                                    <input id="city" name="City"  required type="text" />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>State / County</label>
                                    <input id="state" name="State" type="text" />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>Postcode / ZIP</label>
                                    <input id="postcode" name="zip" type="text" />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>Phone</label>
                                    <input id="phone" name="phone" type="text" />
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <div class="billing-info mb-4">
                                    <label>Email Address</label>
                                    <input  id="email" name="email" type="text" />
                                </div>
                            </div>
                        </div>
                  
                   
                 
                    </div>
                    <div  class=" row">
                        <button id="btn-primary" class=" btn-primary btn mr-3" type="button"
                           
                            onclick="saveAddress(document.getElementById('firstname').value,document.getElementById('lastname').value,document.getElementById('address').value,document.getElementById('country').value,document.getElementById('city').value,document.getElementById('state').value,document.getElementById('postcode').value,document.getElementById('phone').value,document.getElementById('email').value,document.getElementById('extra').value)">save
                            address
                        </button>

                        <button id="btn-primary" class=" btn-primary btn " type="button"
                           
                        onclick="clearAddress()">clear address
                        address
                    </button>


                        <!-- <button id="btn-primary" class=" btn-primary btn" type="button"
                           
                        onclick="saveAddress(document.getElementById('firstname').value,document.getElementById('lastname').value,document.getElementById('address').value,document.getElementById('country').value,document.getElementById('state').value,document.getElementById('postcode').value,document.getElementById('zip').value,document.getElementById('phone').value,document.getElementById('email').value,document.getElementById('extra').value)">save
                        address
                    </button> -->
                    
                    </div>






<br>



                </div>

                
                <div class="col-lg-5 mt-md-30px mt-lm-30px ">



                    <div class="row">
                        <div class="col-md-2">&nbsp;</div>
                        <div class="col-md-12">
                            <div class="row space-16">&nbsp;</div>
                            <div class="row">
    
    
                                <%  locals.address.forEach((element)=>
                                { %>

                                 
                                    <div class="col-md-4">
                                        <div class="thumbnail">
                                            <div class="caption text-left ">
                                                
                                                <input id="adradio" onclick="addadressbar('<%= element.address.firstname %>','<%= element.address.lastname %>','<%= element.address.address %>','<%= element.address.country %>','<%= element.address.city %>','<%= element.address.state %>','<%= element.address.postcode %>','<%= element.address.phone %>','<%= element.address.email %>','<%= element.address.extra %>')" style="height:50px; width:20px;" type="radio" name="address" >
                                                <h4><%= element.address.firstname %>
                                                </h4>
                                            </div>
                                            <div class="caption card-footer ml-4 ">
                                                <ul>
                                                    <li><%= element.address.address %></li>
                                                    <li><%= element.address.city %></li>
                                                    <li><%= element.address.postcode %></li>
                                                    <li><%= element.address.phone %></li>
                                                
                                                </ul>
                                            </div>
                                        </div>
                                    </div>



                                    <%   }) %>
    
                       





                                 



    
                            </div>
                            <div class="col-md-2">&nbsp;</div>
                        </div>
                    </div>











                    <div class="your-order-area">
                        <h3>Your order</h3>
                        <div class="your-order-wrap gray-bg-4">
                            <div class="your-order-product-info">
                                <div class="your-order-top">
                                    <ul>
                                        <li>Product</li>
                                        <li>Total</li>
                                    </ul>
                                </div>
                                <div class="your-order-middle">
                                    <ul>
                                        <li><span class="order-middle-left">Total X <%= locals.CheckoutData.Quantity  %> Products</span> <span class="order-price">₹ <%= locals.CheckoutData.RealPrice  %>  </span></li>
                                        <li><span class="order-middle-left">Discount</span> <span class="order-price">₹ <%= locals.CheckoutData.Discount %>  </span></li>
                                    </ul>
                                </div>
                                <div class="your-order-bottom">
                                    <ul>
                                        <li class="your-order-shipping">Coupon Discount</li>
                                        <% if(locals.CheckoutData.coupon) { %>
                                        <li>₹ <%= locals.CheckoutData.coupon%></li>
                                        <% } else {%>
                                        <li>₹0</li>
                                        <% } %>
                                    </ul>
                                    <ul>
                                        <li class="your-order-shipping">Shipping</li>
                                        <li>Free shipping</li>
                                    </ul>
                                </div>
                                <div class="your-order-total">
                                    <ul>
                                        <li class="order-total">Total</li>
                                        <li>₹<%= locals.CheckoutData.DisPrice  %></li>
                                    </ul>DisPrice
                                </div>
                            </div>
                            
                            <div class="payment-method">
                                <div class="payment-accordion element-mrg">
                                    <div id="faq" class="panel-group">
                                          <input checked  style="height:50px; width:20px; vertical-align: middle;" style="height:50px; width:50px; vertical-align: middle;" type="radio" id="Paypal" name="delivery" value="Paypal">
                                             <label   vertical-align: middle;" for="javascript">Paypal</label>
                                     
                                          <input   style="height:50px; width:20px; vertical-align: middle;" type="radio" id="RazerPAy" name="delivery" value="RazerPAy">
                                          <label   vertical-align: middle;" for="css">Razer Pay</label><br>
                                        
                                          <input  style="height:50px; width:20px; vertical-align: middle;" type="radio" id="COD" name="delivery" value="COD">
                                        <label   vertical-align: middle;" for="html">Cash On Delivery</label><br>
                                  
                                       


      <input   style="height:50px; width:20px; vertical-align: middle;" type="radio" id="wallet" name="delivery" value="wallet">
                                        <label   vertical-align: middle;" for="html">Wallet</label><br>
    <div style="color: blueviolet" >wallet balance : ₹<%=locals.wallet %></div>

                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="Place-order mt-25">
                       <button id="btn-primary" class="btn-primary btn-lg btn-block" >Place Order</button>
                  





                     


                    





                        </div>
                    </div>
                </div>

                
            </div>
        </div>














        
    </div> 










    
</form>


